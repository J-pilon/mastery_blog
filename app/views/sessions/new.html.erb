<%= form_with(model: @user, url: sessions_path, data: {testid: "signin-form"}) do |f| %>
  <%= f.label "Email" %>
  <%= f.text_field :email, placeholder: "Email" %>

  <%= f.label "Password" %>
  <%= f.password_field :password, placeholder: "Password" %>

  <%= f.submit "Sign In", class: "btn-primary mb-4" %>
  <%= link_to "Forgot Password", new_users_password_path %>
<% end %>
