<%= form_tag sessions_path, data: {testid: "signin-form"}, remote: true do %>
  <%= label_tag "Email" %>
  <%= text_field_tag :email, nil, placeholder: "Email" %>
  <%= label_tag "Password" %>
  <%= password_field_tag :password, nil, placeholder: "Password" %>
  <%= submit_tag "Sign In"%>
<% end %>
